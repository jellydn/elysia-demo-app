{
  "name": "elysia-demo-app",
  "version": "1.0.50",
  "module": "server/index.js",
  "scripts": {
    "dev": "run-p -l dev:*",
    "dev:server": "bun run --watch server/index.ts",
    "dev:inspect": "bun --inspect server/index.ts",
    "dev:web": "cd web && bun run dev",
    "format": "prettier --write .",
    "lint": "run-p lint:*",
    "lint:server": "eslint --ext .ts,.js server",
    "lint:web": "eslint --ext .ts,.js web",
    "test": "bun test",
    "typecheck": "run-p typecheck:*",
    "typecheck:server": "tsc --noEmit --project server/tsconfig.server.json",
    "typecheck:web": "tsc --noEmit --project web/tsconfig.json"
  },
  "dependencies": {
    "@elysiajs/cors": "0.7.1",
    "@elysiajs/eden": "0.7.4",
    "@elysiajs/static": "0.7.1",
    "@elysiajs/swagger": "0.7.3",
    "elysia": "latest",
    "pino": "8.16.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.4.6",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@tsconfig/svelte": "5.0.2",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "bun-types": "latest",
    "eslint": "8.51.0",
    "eslint-config-productsway": "1.3.0",
    "eslint-plugin-svelte": "2.34.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "superjson": "2.0.0",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  }
}
