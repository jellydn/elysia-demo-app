{
  "name": "elysia-demo-app",
  "version": "1.0.50",
  "module": "server/index.js",
  "scripts": {
    "dev": "run-p -l dev:*",
    "dev:server": "bun run --watch server/index.ts",
    "server:inspect": "bun --inspect server/index.ts",
    "dev:web": "cd web && bun run dev",
    "postinstall": "cd web && bun install",
    "format": "biome format .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "test": "bun test",
    "typecheck": "run-p typecheck:*",
    "typecheck:server": "tsc --noEmit --project server/tsconfig.server.json",
    "typecheck:web": "tsc --noEmit --project web/tsconfig.json"
  },
  "dependencies": {
    "@elysiajs/cors": "0.8.0",
    "@elysiajs/eden": "0.8.1",
    "@elysiajs/static": "0.8.1",
    "@elysiajs/swagger": "1.0.3",
    "elysia": "latest",
    "elysia-autoroutes": "0.5.0",
    "pino": "8.20.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@tsconfig/svelte": "5.0.4",
    "bun-types": "latest",
    "npm-run-all2": "6.1.2",
    "superjson": "2.2.1",
    "svelte": "4.2.12",
    "svelte-check": "3.6.9",
    "tslib": "2.6.2",
    "typescript": "5.4.4",
    "vite": "5.2.8"
  }
}
